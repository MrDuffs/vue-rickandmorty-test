{
  "arrowParens": "avoid",
  "printWidth": 80,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "useTabs": false,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": [
        "*.yml"
      ],
      "options": {
        "singleQuote": false
      }
    }
  ],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "@",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}